---
import { getCollection } from 'astro:content';
const codeEntries = await getCollection('tutorials');
import Layout from "../layouts/Layout.astro";
---
<Layout title="Formula Trinity Autonomous Docs">
  <div style={{paddingLeft: "30px"}}>
  <style>
      object {
        width: 800px;
      }
      </style>
  <object type="image/svg+xml" data="/ft_docs/system diagram.svg"></object>
  <h2>Departments</h2>
  <ul>
    <li>Perception</li>
    <li>State Estimation</li>
    <li>Path Planning</li>
    <li>Control</li>
    <li>Hardware</li>
    <li>Simulation</li>
  </ul>
  <h2>Tutorials</h2>
  {
<ul>
  {codeEntries.map(e => (<li><a href={`/ft_docs/${e.slug}`}}>{e.data.title}</a></li>))}
</ul>
}
    </div>
</Layout>
